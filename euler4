

def isPalindrome(s):
    ''' check if a number is a Palindrome '''
    s = str(s)
    return s == s[::-1]
c = 0
a = 100
b = 100

for a in range(100,999) and b in range (100,999) :
    s = a*b
    if isPalindrome(s) == True:
        c = s
    
        
        
print a
print b
print c
print s

    
